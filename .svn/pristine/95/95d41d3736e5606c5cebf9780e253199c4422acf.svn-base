
*{margin:0;padding:0;}
li{list-style:none;}
a{text-decoration:none;}
img{border:none;}
.top{line-height:70px;height:70px;margin:0 auto;background:#FFF;width:1200px}
.header-left{padding-left:20px;height:70px;float:left;margin-right:100px;}
.top ul li{margin-top:0;}
.top ul li>a{color:#000;text-decoration:none;width:auto;padding:0 15px;height:70px;line-height:70px;font-size:17px;display:inline-block;text-align:center;outline:0}
.top ul{position:relative;}
.top ul li{float:left;}
.move{width:70px;height: 4px;border-top: 4px solid #00d2ff;box-sizing:border-box;position: absolute;left:265px; top: 52px;transition: left .2s ease-in-out 0s;-webkit-transition: left .2s ease-in-out 0s; -moz-transition: left .2s ease-in-out 0s;-o-transition: left .2s ease-in-out 0s;}
.top ul li:nth-child(1):hover~ .move{left: 265px;}
.top ul li:nth-child(2):hover~ .move{left:345px;width:105px;}
.top ul li:nth-child(3):hover~ .move{left:480px;}
.top ul li:nth-child(4):hover~ .move{left: 580px;width:100px;}
.top ul li:nth-child(5):hover~ .move{left: 710px;}
.top ul li:nth-child(6):hover~ .move{left: 805px;}

#download a{dipaly:block;width:80px;height:70px;float:left;background:url('../images/cloun1.png') no-repeat;}
#download a:hover{dipaly:block;width:80px;height:70px;background:url('../images/cloun2.png') no-repeat;}
.header-right{text-align:center;height:30px;width:180px;float:left;margin-left:10px;}
.header-right>a{width:70px;margin-left:10px;border:1px solid #00d2ff;box-sizing:border-box;height:30px;border-radius:16px;line-height:30px;display:inline-block;font-size:16px;color:#000;font-family:"微软雅黑";}
.header-right>a:hover{line-height:30px;height:30px;border-radius:16px;background:#00d2ff;color:#fff;text-decoration: none;}
.header-right>a.active {background-color: #00d2ff;color: #FFF;}
@keyframes proRotate { 0%   {-webkit-transform: rotateY(90deg) ;}50%  {-webkit-transform: rotateY(180deg);}100% {-webkit-transform:rotateY(360deg);} }
